					</Border>
					<Border Grid.Column="26" Grid.Row="1">
						<TextBlock>Субсидия, %</TextBlock>
					</Border>
					<Border Grid.Column="27" Grid.Row="1">
						<TextBlock>Номер мероприятия ФЦП</TextBlock>
					</Border>
					<Border Grid.Column="28" Grid.Row="1">
						<TextBlock>номер</TextBlock>
					</Border>
				</Grid>
				<DataGrid
					Grid.Row="1"
					Classes="table"
					x:Name="dataGrid"
					Items="{Binding FormList}"
					SelectedItem="{Binding SelectedForm}">
					<interactivity:Interaction.Behaviors>
						<local:DataGridDragSelectionBehavior/>
						<local:DataGridSelectedItemsBehavior SelectedItems="{Binding SelectedForms, Mode=TwoWay}" />
						<local:DataGridDoubleClickBehavior/>
						<local:DeselectDataGridOnClickOutsideBehavior/>
					</interactivity:Interaction.Behaviors>
					<DataGrid.Columns>

						<DataGridTextColumn Binding="{Binding NumberInOrder.Value}"
											Width="40"
											IsReadOnly="True"
											x:CompileBindings="True"
											x:DataType="forms1:Form18">
							<DataGridTextColumn.HeaderTemplate>
								<DataTemplate>
									<Border Classes="headerTemplate">
										<Grid ColumnDefinitions="*,Auto">
											<TextBlock Grid.Column="0" Text="1"/>
											<Button Grid.Column="1"/>
										</Grid>
									</Border>
								</DataTemplate>
							</DataGridTextColumn.HeaderTemplate>
						</DataGridTextColumn>

						<DataGridTemplateColumn>
							<DataGridTemplateColumn.HeaderTemplate>
								<DataTemplate>
									<Border Classes="headerTemplate">
										<Grid ColumnDefinitions="*,Auto">
											<TextBlock Grid.Column="0" Text="2"/>
											<Button Grid.Column="1"/>
										</Grid>
									</Border>
								</DataTemplate>
							</DataGridTemplateColumn.HeaderTemplate>
							<DataGridTemplateColumn.CellTemplate>
								<DataTemplate x:CompileBindings="True" x:DataType="forms1:Form18">
									<TextBox Text="{Binding OperationCode.Value, Mode=TwoWay}"/>
								</DataTemplate>
							</DataGridTemplateColumn.CellTemplate>
						</DataGridTemplateColumn>

						<DataGridTemplateColumn>
							<DataGridTemplateColumn.HeaderTemplate>
								<DataTemplate>
									<Border Classes="headerTemplate">
