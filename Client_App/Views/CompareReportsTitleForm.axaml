                                IsChecked="{Binding ReplaceJurLicoAddress0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,15"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceJurLicoFactAddress0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,15"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceGradeFIO0, Mode=TwoWay}"/>
                            <CheckBox
