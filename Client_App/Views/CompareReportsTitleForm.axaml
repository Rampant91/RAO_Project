                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceInn0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceKpp0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,7"
                                HorizontalAlignment="Center"
