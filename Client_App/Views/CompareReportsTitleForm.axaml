                                Text="{Binding OldOktmo0, Mode=OneTime}"/>
                            <TextBox
                                Height="26"
                                Margin="0,3"
                                IsReadOnly="True"
                                Text="{Binding OldInn0, Mode=OneTime}"/>
                            <TextBox
                                Height="26"
                                Margin="0,3"
                                IsReadOnly="True"
                                Text="{Binding OldKpp0, Mode=OneTime}"/>
                            <TextBox
                                Height="26"
                                Margin="0,3"
                                IsReadOnly="True"
                                Text="{Binding OldOkopf0, Mode=OneTime}"/>
                            <TextBox
                                Height="26"
                                Margin="0,3"
                                IsReadOnly="True"
                                Text="{Binding OldOkfs0, Mode=OneTime}"/>
                        </StackPanel>
                        <!--  Юридическое лицо - CheckBox  -->
                        <StackPanel
                            Grid.Row="1"
                            Grid.Column="2"
                            HorizontalAlignment="Center">
                            <CheckBox
                                Margin="0,4,0,7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceSubjectRF0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,15"
                                HorizontalAlignment="Center"
