                                IsReadOnly="True"
                                ScrollViewer.VerticalScrollBarVisibility="Hidden"
                                Text="{Binding OldJurLico1, Mode=OneTime}"
                                TextWrapping="Wrap"/>
                            <TextBox
                                Height="26"
                                Margin="0,3"
