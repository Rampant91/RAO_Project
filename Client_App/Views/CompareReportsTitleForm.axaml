<Window
    x:Class="Client_App.Views.CompareReportsTitleForm"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:tools="clr-namespace:Client_App.Tools"
    xmlns:type="clr-namespace:Client_App.Tools.ConverterType"
    xmlns:vm="clr-namespace:Client_App.ViewModels"
    Title="{Binding Title, Mode=OneTime}"
    Width="{Binding Source={x:Type type:Width}, Converter={tools:RatioConverter}, ConverterParameter='0.5', FallbackValue='600', Mode=OneTime}"
    Height="{Binding Source={x:Type type:Height}, Converter={tools:RatioConverter}, ConverterParameter='0.75', FallbackValue='800', Mode=OneTime}"
    MinWidth="950"
    MinHeight="600"
    x:CompileBindings="True"
    x:DataType="vm:CompareReportsTitleFormVM"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">

    <Design.DataContext>
        <vm:CompareReportsTitleFormVM/>
    </Design.DataContext>

    <Grid>
        <!--  TopTable  -->
        <Grid.RowDefinitions>
            <RowDefinition Height="*" MaxHeight="200"/>
            <RowDefinition Height="2*" MinHeight="400"/>
        </Grid.RowDefinitions>
        <Border
            Grid.Row="0"
            MinHeight="200"
            MaxHeight="200"
            Margin="5,5,5,0"
            Padding="0,5,0,0"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            BorderBrush="Black"
            BorderThickness="1"
            CornerRadius="5">
            <Grid ColumnDefinitions="250,400" RowDefinitions="*,*,*,*,*">
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="5,3,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="Регистрационный номер:"/>
                <TextBox
                    Grid.Row="0"
                    Grid.Column="1"
                    Width="300"
                    Height="30"
                    Margin="0,3,5,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    IsReadOnly="True"
                    Text="{Binding RegNo, Mode=OneTime}"/>
                <TextBlock
                    Grid.Row="1"
                    Grid.Column="0"
                    Margin="5,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="Сокращенное наименование:"/>
                <TextBox
                    Grid.Row="1"
                    Grid.Column="1"
                    Width="300"
                    Height="30"
                    Margin="0,0,5,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    IsReadOnly="True"
                    Text="{Binding ShortJurLico, Mode=OneTime}"/>
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Margin="5,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="ОКПО:"/>
                <TextBox
                    Grid.Row="2"
                    Grid.Column="1"
                    Width="300"
                    Height="30"
                    Margin="0,0,5,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    IsReadOnly="True"
                    Text="{Binding Okpo, Mode=OneTime}"/>
                <WrapPanel
                    Grid.Row="3"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Margin="20,30,0,20">
                    <Button
                        Name="Replace"
                        Width="80"
                        Height="30"
                        Margin="0,0,20,0"
                        Click="ReplaceButtonClick"
                        Content="Заменить"/>
                    <Button
                        Name="CloseWithoutReplace"
                        Width="80"
                        Height="30"
                        Margin="0,0,20,0"
                        Click="CloseWithoutReplaceButtonClick"
                        Content="Не заменять"/>
                    <Button
                        Name="CloseWithoutReplaceForThisReps"
                        Width="150"
                        Height="30"
                        Margin="0,0,143,0"
                        Click="CloseWithoutReplaceForThisRepsButtonClick"
                        Content="Не заменять у этой орг."/>
                    <Button
                        Name="SelectAll"
                        Width="80"
                        Height="30"
                        Click="SelectAllButtonClick"
                        Content="Выбрать всё"/>
                </WrapPanel>
                <StackPanel
                    Grid.Row="4"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    VerticalAlignment="Bottom"
                    Orientation="Horizontal">
                    <TextBlock Margin="320,0,0,0" Text="Значения в базе"/>
                    <TextBlock Margin="117,0,0,0" Text="Заменить"/>
                    <TextBlock Margin="85,0,0,0" Text="Импортируемые значения"/>
                </StackPanel>
            </Grid>

