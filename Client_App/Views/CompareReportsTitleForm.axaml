                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceTelephone0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceFax0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceEmail0, Mode=TwoWay}"/>
                            <CheckBox
                                Margin="0,7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                IsChecked="{Binding ReplaceOkpo0, Mode=TwoWay}"/>
                            <CheckBox
