<Application x:Class="Client_App.App"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Client_App"
             xmlns:v="clr-namespace:Client_App.Views"
             xmlns:vm="clr-namespace:Client_App.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:AnnualReportsVM}">
                <v:AnnualReportsView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:OperReportsVM}">
                <v:OperReportsView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>

    
</Application>